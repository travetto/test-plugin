{
  "name": "encore2-test-plugin",
  "displayName": "Encore2 Test Plugin",
  "description": "UI integration for encore2 testing",
  "version": "0.0.1",
  "publisher": "eaitechnologies",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "workspaceContains:**/node_modules/@encore2/test"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "files": [
    "src/extension.ts"
  ],
  "devDependencies": {
    "typescript": "^2.5.2",
    "vscode": "^1.1.5",
    "@types/node": "^8.0.0"
  },
  "dependencies": {
    "@encore2/test": "git+ssh://git@git.eaiti.com/encore2/test.git#v1.x.x"
  }
}